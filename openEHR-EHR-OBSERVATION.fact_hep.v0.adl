	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"The FACT-Hep consists of 45 questions: 27 from Functional Assessment of Cancer Therapy-General (FACTG), assessing physical, emotional, functional, and social/family functioning, and 18 from the site-specific hepatobiliary subscale (HBS). Total scores are calculated for the HBS, FACT-G Trial Outcome Index (TOI; sum of physical, functional, and disease-specific [HBS in this case] subscales), and FACT-Hep (ie, HBS plus FACT-G).">
			keywords = <"FACT-Hep, cancer, QOL, quality of life, Hepatobiliary, Therapy-Hepatobiliary, assessment, liver, bile duct, pancreas", ...>
			use = <"To record the cancer specific measures of patients with hepatobiliary cancer. Original FACT-HEP assessment can be found at https://www.facit.org/LiteratureRetrieve.aspx?ID=42269
It is based on the version 4.">
